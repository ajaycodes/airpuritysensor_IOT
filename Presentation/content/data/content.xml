      <width>-92.46</width>
      <p>
        <text weight="bold"><![CDATA[Solution]]></text>
      </p>
    </object>
    <object id="58_34813587" type="persistentgroup" x="1613.9534157669775" y="-1636.6733986203587" r="57.99618652009111" s="1.2045263500080379">
      <m o="49_34813587"/>
